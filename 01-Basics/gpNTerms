package loops;

import java.util.Scanner;

public class gpNTerms {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);//Gp:-1,2,4,8,16....up to n
        System.out.println("enter the number:");
        int n=sc.nextInt();
        int a=1;
        for(int i=1;i<=n;i++){
            System.out.println(a);
            a*=2;
        }
    }
}
